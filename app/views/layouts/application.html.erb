<!DOCTYPE html>
<html>
  <head>
    <title>Translogic International</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar navbar-default navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <button type="button" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar" class="navbar-toggle collapsed"><span class="sr-only">Toggle navigation</span><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span></button><a href="/" class="navbar-brand">Translogic International</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse navbar-right">
          <ul class="nav navbar-nav">
            <li><%= link_to 'Home', root_path %></li>
            <li><%= link_to 'About Us', about_path %></li>
            <li><%= link_to 'Contact Us', contact_path %></li>
            <li><%= link_to 'Track Package', track_path %></li>
            <% if user_signed_in? %>
            <li><%= link_to 'Log Out', destroy_user_session_path, method: :delete %></li>
            <li><%= link_to 'Shipments', shipments_path %></li>
            <% end %>
          </ul>
        </div>
      </div>
    </nav>
        <%= yield %>
        <p class="notice"><%= notice %></p>
         <p class="alert"><%= alert %></p>
  </body>
  <footer>
  <p>&copy; 2016. [All Rights Reserved]</p>
</footer>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js">

</script>
</html>
